{
  "name": "drexel-tms-parser",
  "version": "0.1.0",
  "description": "Made to parse out the Drexel TMS to make class selection easier to manage.",
  "private": true,
  "scripts": {
    "nuke:all": "npm run nuke:py; npm run nuke:node; bash ./install.sh",
    "nuke:node": "rm -fr ./**/node_modules",
    "nuke:py": "rm -fr ./**/venv",
    "webapp": "PORT=5000 python ./src/webapp.py",
    "serve": "vue-cli-service serve",
    "dev": "NODE_ENV=dev vue-cli-service build --watch",
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service build --modern --target app"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "register-service-worker": "^1.5.2",
    "vue": "^2.5.22",
    "vuetify": "^1.4.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@gfx/zopfli": "^1.0.10",
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-plugin-pwa": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "ajv": "^6.6.2",
    "autoprefixer": "^9.4.5",
    "babel-eslint": "^10.0.1",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.28.4"
  },
  "engines": {
    "node": ">= 10.5.0",
    "npm": ">= 3.0.0"
  }
}
